{{#partial "content"}}
    <p>Here are all the units listed with the students score:</p>
    <ul>
        {{#each units}}

                <p>
                    <a href="/students/{{id}}/units{{unit.studentId}}">{{math}}{{english}}{{kiswahili}}{{science}}{{socialStudy}}{{cre}}{{studentId}}</a><br>
                </p>

        {{/each}}
    </ul>
    <button>    <a href="/units/{{unit.studentId}}/edit">Edit units</a><br>
    </button>
    <button>
        <a href="/units/{{unit.studentId}}/delete">DELETE this units</a>
    </button>
{{/partial}}

{{> layout.hbs}}